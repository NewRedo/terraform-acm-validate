{
  "name": "${localWorkspaceFolderBasename}",
  "dockerComposeFile": "docker-compose.yml",
  "service": "dev",
  "workspaceFolder": "/workspace",
  "remoteUser": "newredo",
  "mounts": [
    "source=${env:HOME}${env:USERPROFILE}/.aws,target=/home/newredo/.aws,type=bind,consistency=cached"
  ],
  "customizations": {
    "vscode": {
      "extensions": ["hashicorp.terraform"]
    }
  }
}
